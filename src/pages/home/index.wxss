.home {
  min-width: 100vw;
  min-height: 100vh;
}
.home-header {
  width: 100%;
  min-height: 100%;
  display: flex;
  position: relative;
}
.home-header .header-img {
  width: 32px;
  height: 32px;
  position: absolute;
  top: 20px;
  z-index: 10;
}
.home-header .header-list {
  left: 20px;
}
.home-header .header-search {
  right: 20px;
}
.home .am-tabs-tab-bar-wrap {
  height: 70px;
  width: 80%;
  margin: 0 auto;
}
.home .am-tabs-default-bar-tab {
  height: 70px;
}
.home .am-tabs-default-bar-tab::after {
  display: none !important;
}
.home .am-tabs-default-bar-underline {
  display: none;
}
.home .drawer {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  overflow: auto;
  width: 100%;
  height: 100%;
  z-index: 100;
  -webkit-overflow-scrolling: touch;
}
